<?xml version="1.0" encoding="utf-8"?>
<manifest manifest-version="0.1" 
  xmlns="http://www.tableau.com/xml/extension_manifest">
  <dashboard-extension id="com.theinformationlab.extensions.imageviewer" extension-version="1.0.0">
    <default-locale>en_US</default-locale>
    <name resource-id="name"/>
    <description>Data-driven Image Viewer for Tableau Dashboards</description>
    <author name="Craig Bloodworth" email="craig.bloodworth@theinformationlab.co.uk" organization="The Information Lab" website="https://www.theinformationlab.co.uk"/>
    <min-api-version>0.8</min-api-version>
    <source-location>
      <url>https://imageviewerextension.theinformationlab.io/viewer.html</url>
      <!-- <url>http://localhost:8000/viewer.html</url> -->
    </source-location>
    <icon></icon>
    <context-menu>
      <configure-context-menu-item />
    </context-menu>
  </dashboard-extension>
  <resources>
    <resource id="name">
      <text locale="en_US">Image Viewer</text>
    </resource>
  </resources>
</manifest>
